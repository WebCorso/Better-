    <!-- Your website content goes here -->
    <script>
        // script.js
const progressBar = document.querySelector('.progress-bar');
const progressContainer = document.querySelector('.progress-container');

window.addEventListener('scroll', () => {
    const windowHeight = window.innerHeight;
    const totalHeight = document.body.clientHeight - windowHeight;
    const scrolled = window.scrollY;
    const progressWidth = (scrolled / totalHeight) * 100;
    progressBar.style.width = `${progressWidth}%`;
});

// Update progress bar on page load (in case the page is not scrolled to the top)
document.addEventListener('DOMContentLoaded', () => {
    const progressWidth = (window.scrollY / (document.body.clientHeight - window.innerHeight)) * 100;
    progressBar.style.width = `${progressWidth}%`;
});

    </script>
</body>
</html>
